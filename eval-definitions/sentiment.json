                    "Registry": "sysdevtamu/sentiment-cfail:latest"
                },
                "SENTIMENT_DB": {
                    "Registry": "sysdevtamu/sentiment-db:latest"
                },
                "SENTIMENT_MAIN": {
                    "Registry": "sysdevtamu/sentiment-main:latest",
                    "Endpoints": ["SENTIMENT_READ_CSV"]
                },
                "SENTIMENT_PRODUCT_OR_SERVICE": {
                    "Registry": "sysdevtamu/sentiment-product-or-service:latest",
                    "Endpoints": ["SENTIMENT_CFAIL", "SENTIMENT_PRODUCT_SENTIMENT", "SENTIMENT_SERVICE_SENTIMENT"]
                },
                "SENTIMENT_PRODUCT_RESULT": {
                    "Registry": "sysdevtamu/sentiment-product-result:latest",
